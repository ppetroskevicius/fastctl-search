                  ]
                }
              },
              "required": [
                "name"
              ]
            },
            "description": "Field type: ."
          }
        },
        "required": [
          "lines",
          "station_name",
          "walk_time_min"
        ]
      },
      "description": "Field type: ."
    },
    "other_requirements": {
      "type": "object",
      "properties": {
        "japanese_required": {
          "type": "boolean",
          "description": "Field type: boolean. Example: True"
        },
        "note": {
          "type": "string",
          "description": "Field type: string."
        }
      },
      "required": [
        "japanese_required",
        "note"
      ],
      "description": "Field type: ."
    },
    "unit_notes": {
      "type": "string",
      "description": "Field type: string."
    }
  },
  "required": [
    "id",
    "url",
    "name",
    "property_type",
    "address",
    "area",
    "type",
    "price",
    "images",
    "initial_cost_estimate"
  ]
